#include "dog.h"

/**
 * new_dog - constructs a new dog of type dog_t
 * @name: name
 *@age: age
 *@owner: owner
 * Returns: dog_t
 */

dog_t *new_dog(char *name, float age, char *owner)
{
	if (new_dog == NULL)
		return (NULL);

	*new_dog->name = name;
	*new_dog->age = age;
	*new_dog->owner = owner;

	return (dog_t);
}
